public class Main
{
	public static void main(String[] args) {
	    int n=5;
	    int j=1;
	for(int i=1;i<=n;i++){
	  for(int k=0;k<i;k++){
	      System.out.print(j+" ");
	      j++;
	  }System.out.println();
	}
	}
}
